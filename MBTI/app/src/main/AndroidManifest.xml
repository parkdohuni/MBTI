<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mbti_roommate">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Match" />
        <activity android:name=".Home" />
        <activity android:name=".UserRegister" />
        <activity android:name=".BottomNavigationActivity" />
        <activity android:name=".IndividualProfileResult" />
        <activity android:name=".MatchResult" />
        <activity android:name=".MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SearchProfile"/>
        <activity android:name=".DenyActivity"/>
        <activity android:name=".ProfileEditActivity"/>
        <activity android:name=".RequestActivity"/>
        <activity android:name=".ResponseActivity"/>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>